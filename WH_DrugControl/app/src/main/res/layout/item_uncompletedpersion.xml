<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="16dp">

    <TextView
        android:id="@+id/uncompleted_sex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:textColor="#333"
        android:textSize="16sp"
        tools:text="ç”·"/>

    <TextView
        android:id="@+id/uncompleted_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        android:layout_toLeftOf="@id/uncompleted_sex"
        android:lines="1"
        android:textColor="#333"
        android:textSize="16sp"
        tools:text="@string/app_name"/>

    <TextView
        android:id="@+id/uncompleted_idcard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/uncompleted_name"
        android:layout_below="@id/uncompleted_name"
        android:layout_marginTop="16dp"
        android:layout_toLeftOf="@id/uncompleted_sex"
        android:textColor="#ababab"
        android:textSize="14sp"
        tools:text="@string/sfzhmlable"/>

</RelativeLayout>
