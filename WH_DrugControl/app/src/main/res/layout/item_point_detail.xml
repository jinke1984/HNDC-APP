<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:baselineAligned="false"
    android:orientation="vertical"
    android:paddingBottom="5dp"
    android:paddingTop="5dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="6.6dp">

        <TextView
            android:id="@+id/jflx_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="6.6dp"
            android:text="@string/jflx"
            android:textColor="@color/messge_control_text_color"
            android:textSize="13.3sp" />

        <TextView
            android:id="@+id/dqjf_tv"
            android:layout_width="64dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:gravity="center|left"
            android:text="@string/dqjf"
            android:textColor="@color/messge_control_text_color"
            android:textSize="13.3sp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/jfjsrq_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6.6dp"
        android:layout_marginLeft="6.6dp"
        android:layout_marginTop="6.6dp"
        android:text="@string/jfjsrq"
        android:textColor="@color/messge_control_text_color"
        android:textSize="13.3sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6.6dp"
        android:background="@color/white"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6.6dp"
            android:text="@string/jfmx"
            android:textColor="@color/messge_control_text_color"
            android:textSize="13.3sp" />

        <TextView
            android:id="@+id/jfmx_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/jfmx"
            android:textColor="@color/messge_control_text_color"
            android:textSize="13.3sp" />
    </LinearLayout>
</LinearLayout>
